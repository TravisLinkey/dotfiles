# App Shortcuts
alt - c : open -a 'Cursor'
alt - w : open -a 'google chrome'
alt - t : open -na 'Kitty'
alt - v : open -a 'Visual Studio Code'
alt - o : open -a 'Obsidian'
alt - p : open -a 'postman'
alt - b : open -a 'bruno'
alt - u : open -a firefox "https://www.udemy.com/course/google-cloud-certification-associate-cloud-engineer/"

# focus window
alt -h : yabai -m window --focus west
alt -j : yabai -m window --focus south
alt -k : yabai -m window --focus north
alt -l : yabai -m window --focus east

# toggle window zoom
alt -g : yabai -m window --toggle windowed-fullscreen
alt -f : yabai -m window --toggle zoom-fullscreen

# toggle window split type
alt -e : yabai -m window --toggle split
